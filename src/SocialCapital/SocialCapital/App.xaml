<?xml version="1.0" encoding="UTF-8"?>
<Application xmlns="http://xamarin.com/schemas/2014/forms" 
			 xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
			 xmlns:converters="clr-namespace:SocialCapital.Views.Converters;assembly=SocialCapital"
			 x:Class="SocialCapital.App">
	<Application.Resources>
		<ResourceDictionary>
        	<converters:NegativeBooleanConverter x:Key="not" />
        	<converters:ContactToStatusConverter x:Key="StatusConverter" />
        	<converters:ListToBoolConverter x:Key="ListToBool" />
        	<converters:DateTimeCustomConverter x:Key="DateTimeCustom" />
        	<converters:CommunicationToImageConverter x:Key="CommunicationToImage" />
        	<converters:PeriodConverter x:Key="PeriodConverter" />
        	<converters:ListToCountConverter x:Key="ListToCountConverter" />
        	<converters:BoolToImageConverter x:Key="BoolToImageConverter" />
        	<converters:FrequencyConverter x:Key="FrequencyConverter" />
        	<converters:StringToBoolConverter x:Key="StringToBoolConverter" />

        	<Style x:Key="TextCellStyle" TargetType="TextCell">
        		<Setter Property="TextColor" Value="Black" />
        	</Style>
    	</ResourceDictionary>
	</Application.Resources>
</Application>
